    "@testing-library/dom": "^10.4.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "immer": "^10.1.1",
    "jspdf": "^3.0.0",
